{
  "name": "vue2-Koa2-es6-webpack2",
  "version": "1.0.0",
  "keywords": [
    "vue2",
    "Koa2",
    "es6",
    "webpack2"
  ],
  "description": "vue2 SPA Framework",
  "repository": {
    "type": "git",
    "url": "https://github.com/vue-demo/vue2spa.git"
  },
  "author": "vmto <vmto@qq.com>",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "webpack-dev-server --open --inline --hot ",
    "build": "webpack --progress --hide-modules",
    "webpackdev": "better-npm-run webpack:dev",
    "webpackprod": "better-npm-run webpack:prod"
  },
  "betterScripts": {
    "webpack:dev": {
      "command": "webpack --progress --colors",
      "env": {
        "NODE_ENV": "dev"
      }
    },
    "webpack:prod": {
      "command": "webpack --progress --colors",
      "env": {
        "NODE_ENV": "prod"
      }
    }
  },
  "dependencies": {
    "vue": "^2.1.4",
    "vue-resource": "^1.0.3",
    "vue-router": "^2.1.1",
    "vuex": "^2.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.19.0",
    "babel-preset-es2015": "^6.13.2",
    "better-npm-run": "^0.0.13",
    "cross-env": "^2.0.0",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.1",
    "url-loader": "^0.5.7",
    "vue-loader": "^10.0.2",
    "vue-style-loader": "^1.0.0",
    "vue-template-compiler": "^2.1.4",
    "webpack": "2.1.0-beta.27",
    "webpack-dev-server": "^1.16.2",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.13.2"
  }
}
